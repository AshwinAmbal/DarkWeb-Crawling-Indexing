<script>
    var trackClick = function (service) {
        var jesterObj = {
            verb: 'click',
            object_type: 'link',
            object_id: service,
            product: 'dotcom',
            zone: 'login',
            section: service,
        };

        

        window.DISQUS.jester.event(jesterObj);
    };

    $('body').delegate('.social-button', 'click', function () {
        trackClick(this.getAttribute('data-service'));
    });
</script>


