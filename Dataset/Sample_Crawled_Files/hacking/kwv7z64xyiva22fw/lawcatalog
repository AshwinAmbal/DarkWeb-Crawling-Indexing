        <script type="text/javascript" language="javascript">
            function openSearchUrl() {
                var ntt = document.getElementsByName('Ntt')[0].value;
                var sitePromo = document.getElementById('hdnSitePromoCode').value;
                url = "http://www.newyorklawjournal.com/default-search-page-layout?query=" + ntt + "&publication=New+York+Law+Journal&x=0&y=0";
                if (sitePromo == "NJ") {
                    url = "http://www.njlawjournal.com/search-results-layout-page?query=" + ntt + "&publication=New+Jersey+Law+Journal&x=0&y=0";
                }
                else if (sitePromo == "CT") {
                    url = "http://www.ctlawtribune.com/search-results-layout-page?query=" + ntt + "&publication=Connecticut&x=0&y=0";
                }
                else if (sitePromo == "PA") {
                    url = "http://www.thelegalintelligencer.com/search-results-layout-page?query=" + ntt + "&publication=The+Legal+Intelligencer&x=0&y=0";
                }
                else if (sitePromo == "FLM") {
                    url = "http://www.dailybusinessreview.com/search-results-layout-page?query=" + ntt + "&publication=Daily+Business+Review&x=0&y=0";
                }
                else if (sitePromo == "NY") {
                    url = "http://www.newyorklawjournal.com/default-search-page-layout?query=" + ntt + "&publication=New+York+Law+Journal&x=0&y=0";
                }
                else if (sitePromo == "TAL") {
                    url = "http://www.americanlawyer.com/search-results-layout-page?query=" + ntt + "&publication=TAL+2008&x=9&y=13";
                }
                else if (sitePromo == "CC") {
                    url = "http://www.corpcounsel.com/default-search-results-layout?query=" + ntt + "&publication=CC+Corporate+Counsel&x=4&y=11";
                }
                else if (sitePromo == "CA") {
                    url = "http://www.therecorder.com/search-results?query=" + ntt + "&publication=The+Recorder&x=14&y=4";
                }
                else if (sitePromo == "NL") {
                    url = "http://www.nationallawjournal.com/default-search?query=" + ntt + "&publication=National+Law+Journal&x=5&y=8";
                }
                else if (sitePromo == "TX") {
                    url = "http://www.texaslawyer.com/search-results-layout-page?query=" + ntt + "&publication=Texas+Lawyer&x=8&y=10";
                }
                else if (sitePromo == "DBCI") {
                    url = "http://www.delbizcourt.com/search-results-layout-page?query=" + ntt + "&publication=Delaware+Business+Court+Insider&x=10&y=12";
                }
                else if (sitePromo == "LIT") {
                    url = "http://www.litigationdaily.com/search-results?query=" + ntt + "&publication=Litigation+Daily&x=10&y=6";
                }
                else if (sitePromo == "GA") {
                    url = "http://www.dailyreportonline.com/search-results-layout-page?query=" + ntt + "&publication=Daily+Report&x=0&y=0";
                }
                else if (sitePromo == "DLW") {
                    url = "http://www.delawarelawweekly.com/search-results-layout-page?query=" + ntt + "&publication=Delaware+Law+Weekly&x=14&y=14";
                }
                else if (sitePromo == "LT") {
                    url = "http://www.lawtechnologynews.com/search-results-layout-page?query=" + ntt + "&publication=Legal+Technology&x=10&y=8";
                }
                document.getElementsByName('Ntt')[0].value = '';
                window.open(url, "_blank");
                return false;
            }
        </script>
        

