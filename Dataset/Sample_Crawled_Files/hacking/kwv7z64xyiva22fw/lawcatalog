            function toggle_visibility(id) {
                var e = document.getElementById(id);
                var r = document.getElementById(id).childNodes[2];
                if (e.style.display == 'block')
                    e.style.display = 'none';
                else
                    e.style.display = 'block';
            }


            function rotate_icon(id) {
                var r = document.getElementById(id);
                if (r.style.transform == 'rotate(180deg)')
                    r.style.transform = 'rotate(0deg)';
                    /*r.style.css('transform', 'rotate(45deg)');
                    e.('.rotate-me').style.transform = 'rotate(0deg)';*/
                else
                    r.style.transform = 'rotate(180deg)';
                /*e.('.rotate-me').style.transform = 'rotate(180deg)';*/
            }


        </script>
