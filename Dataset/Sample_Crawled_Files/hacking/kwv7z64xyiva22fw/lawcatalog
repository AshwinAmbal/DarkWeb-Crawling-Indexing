<script language="javascript" type="text/javascript">

    function pageLoad() {

        var hf = document.cookie.split("=");
        if (hf == "") { return; }
        if (hf[1] == "Hidden") {
            //var modpop = $find("ctl00_ctl00_MainContent_OnColumnMainContent_CustomLandingPageViewControl1_mpcAddToCart_mpe");
            //if (!modpop) { return; }
            //modpop.hide();
            //document.cookie = "Hidden=";
        }
    }

    function SetSelection(ctrlID) {
        var ctrl = document.getElementById(ctrlID);
        if (ctrl != null) {
            ctrl.className = "active";
        }
        return false;
    }
    
    function launchWindownew(objhref) {
        var left = (screen.width / 2) - (550 / 2);
        var top = (screen.height / 2) - (550 / 2);
        objhref = objhref.replace("~/", "/");
        bSuccess = window.open(objhref, "mywindow", "menubar=1,resizable=1,width=650,height=550");
        bSuccess.moveTo(left, top);
        // The window was opened, so stop the browser processing further
        return false;
    }

</script>

