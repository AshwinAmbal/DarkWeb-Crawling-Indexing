        /* Login Overlay Load Function - s.prop20, event85 */
        function scoverlay() {
            var s = s_gi(s_account);
            s.linkTrackVars = "eVar20,prop20,events";
            s.linkTrackEvents = "event85";
            s.prop20 = "subscriber login";
            s.eVar20 = "subscriber login";
            s.events = "event85";
            s.tl(this, 'o', 'load login overlay');
        }

        /* Login Overlay Close Function - event87 */
        function scOverlayClose() {
            var s = s_gi(s_account);
            s.linkTrackVars = "eVar20,events";
            s.linkTrackEvents = "event87";
            s.eVar20 = "subscriber login";
            s.events = "event87";
            s.tl(this, 'o', 'close login overlay');
        }



