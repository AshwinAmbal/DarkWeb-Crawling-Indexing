<script type='text/javascript'>

	
	$(document).ready(function(){

		$('#extra-nav-toggle a').click(function(){
			if($(this).hasClass('open')){
				$('#extra-nav').hide();
				$(this).removeClass('open');
				$('#extra-nav-toggle a').removeClass('ss-delete');
				$('#extra-nav-toggle a').addClass('ss-dropdown');		
			} else {
				$('#extra-nav').show();
				$(this).addClass('open');
				$('#extra-nav-toggle a').removeClass('ss-dropdown');
				$('#extra-nav-toggle a').addClass('ss-delete');					
			}
			return false;
		});
		
		var isMobile = window.matchMedia("only screen and (min-width: 60.625em)");

    	if (isMobile.matches) {	
    		
			$('li.dealsNav').mouseenter(function(){
			    $(".dealsList").css({ 'display':'block'});
			});           
		
			$('ul.dealsList').mouseleave(function(){  
			    $(".dealsList").css({ 'display':'none'});	    
			});
		
		}
		  				  
	});//-- end document Ready
		
	


