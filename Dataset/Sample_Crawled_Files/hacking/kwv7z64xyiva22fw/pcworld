		<script>
			function bindPerformanceMarks(){
			//even though it's called slotRenderEnded, it's the first thing to happen -  corresponds to when we get a response back from DFP
			googletag.pubads().addEventListener('slotRenderEnded', function(event) {
				var slotId = event.slot.getSlotElementId();
				//console.log("slotId = "+slotId);
								
				if (slotId == 'gpt-superstitial' && !event.isEmpty) {//also ensure it's not an empty ad response
					performance.mark("mark_welcome_render_begin");
				}
			});
			googletag.pubads().addEventListener('slotOnload', function(event) {
				var slotId = event.slot.getSlotElementId();
				//console.log("slotId = "+slotId);
								
				if (slotId == 'gpt-superstitial') {
					performance.mark("mark_welcome_render_end");
				}
			});
			googletag.pubads().addEventListener('impressionViewable', function(event) {
				var slotId = event.slot.getSlotElementId();
				//console.log("slotId = "+slotId);
								
				if (slotId == 'gpt-superstitial') {
					performance.mark("mark_welcome_impression_viewable");
									
				}
			});
				}
				try {
					IDG.GPT.googleCommandPush(bindPerformanceMarks);
				}
				catch(e){
					console.log('error binding performance marks: ' + e);
				}
		</script>
	
	
