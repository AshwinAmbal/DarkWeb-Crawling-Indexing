    <link id="stylelink" rel="preload" href="/_qs/static/dist/css/main.cf7ba0b4f228adf14464107ac0db2a71.css" as="style">
    
    <script>
      (function() {
        var DOMTokenListSupports = function(tokenList, token) {
          if (!tokenList || !tokenList.supports) {
            return;
          }
          try {
            return tokenList.supports(token);
          } catch (e) {
            return false;
          }
        };
        var styleLink = document.getElementById('stylelink');
        var isPreloadSupported = DOMTokenListSupports(document.createElement('link').relList, 'preload');

        function hasThisUrlLoaded(url) {
          var absoluteURL = new URL(url, document.baseURI).href;
          return !!window.performance.getEntriesByName(absoluteURL).length;
        }

        function loadCss() {
          styleLink.rel = 'stylesheet';
        }

        if (isPreloadSupported) {
          if (hasThisUrlLoaded(styleLink.href)) {
            loadCss();
          } else {
            styleLink.addEventListener('load', loadCss);
          }
        } else {
          var s = document.createElement('link');
          s.href = styleLink.href;
          s.rel = 'stylesheet';
          document.head.appendChild(s);
        }
      })();
    </script>
  </head>

