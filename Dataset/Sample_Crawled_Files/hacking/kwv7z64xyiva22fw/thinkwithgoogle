              { "url": "/_qs/static/dist/js/vendor.e8e21d061234e882fe3c.js",
                "options": {
                  "async": false,
                  "defer": false
                }
              },
              { "url": "/_qs/static/dist/js/main.c995425c11cdfaa984fb.js",
                "options": {
                  "async": false,
                  "defer": false
                }
              }
            ]'></div>

    <script>
      var base_scripts = document.querySelector("#base-scripts").getAttribute('data-scripts');
      base_scripts = JSON.parse(base_scripts);
      base_scripts  .forEach(function(scriptObj) {
        var s = document.createElement('script');
        s.async = scriptObj.options.async;
        s.defer = scriptObj.options.defer;
        s.src = scriptObj.url;
        document.body.appendChild(s);
      });
    </script>

    
      
    
  </body>
</html>



