<div class="search-bar desktop"
  data-sitespace=""
  ng-controller="SearchBar as searchBar"
  ng-init="searchBar.breakpoint = 'desktop'"
  uni-click-outside="searchBar.closeSearchBar('desktop')"
  ng-cloak>
  <form class="search-bar__form"
    ng-submit="searchBar.onSubmit()">
    <input type="text" class="search-bar__input desktop"
      placeholder="Find an article..."
      aria-label="Find an article..."
      ng-model='searchBar.searchInput.model'
      ng-model-options='{ debounce: 200 }'
      ng-change='searchBar.onSearchChange(searchBar.searchInput.model)'
      ng-keydown='searchBar.onKeyDown($event)'
      ng-focus='searchBar.onInputFocus()'
      ng-blur='searchBar.onInputBlur()'
      />
    <div class="uni-type-ahead desktop">
      <a href="{[ result.url ]}" class="type-ahead-list-item"
        ng-repeat="result in searchBar.searchResults"
        ng-keydown='searchBar.onKeyDown($event)'
        ng-click="searchBar.closeSearchBar()"
        ng-focus="searchBar.onItemFocus($index)"
        >
        <span class="h-c-eyebrow events-off" ng-bind-html="result.client_eyebrow | safe"></span>
        <h4 class="events-off">{[ result.headline ]}</h4>
      </a>
    </div>
    <button type="submit" value="Submit" class="search-bar__button search-bar__button--search" aria-label="Search">
      <svg role="img" class="h-c-icon h-c-icon--color-text events-off">
        <use xlink:href="#mi-search"></use>
      </svg>
