function randOrd() {
    return (Math.round(Math.random())-0.5); 
}

$(document).ready(function() {
    var klasses = [ 'bg1', 'bg2', 'bg3', 'bg4' ];
    klasses.sort( randOrd );
    $('#topImage').each(function(i, val) {
        $(this).addClass(klasses[i]);
    });
});

