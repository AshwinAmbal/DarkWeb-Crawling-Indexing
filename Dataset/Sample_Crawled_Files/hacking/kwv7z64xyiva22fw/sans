        <input data-drupal-selector="edit-gdpr-notice-accept-no" type="radio" id="edit-gdpr-notice-accept-no" name="gdpr_notice[accept]" value="No" class="form-radio" />

        <label for="edit-gdpr-notice-accept-no" class="option">No</label>
