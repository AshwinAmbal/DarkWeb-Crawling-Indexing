    $(document).ready(function(){
         if($(".hidValidPassword").val() != 1)
         {
            $("#layout1HeaderLoginDiv").find("input, div").hide();
            $("#layout1HeaderLoginButtonDiv").find("div[id$=btnLogin]").find("span").hide();
            $("#layout1HeaderLoginButtonDiv").find("div[id$=btnLogin]").attr("style", "display:none!important");
            $("#loginMainDiv").find("a[id$=forgotPwdLink]").attr("style", "display:none!important");
        }
        else
        {
            $("#layout1HeaderLoginDiv").find("a").hide();
        }

        $("#layout1HeaderLoginDiv").click(function(){
            $("#layout1HeaderLoginDiv").find("input, div").show();
            $("#layout1HeaderLoginDiv").find("a").hide();
            $("#layout1HeaderLoginButtonDiv").find("div[id$=btnLogin]").find("span").show();
            $("#layout1HeaderLoginButtonDiv").find("div[id$=btnLogin]").attr("style", "display:inline-block!important");
            $("#loginMainDiv").find("a[id$=forgotPwdLink]").attr("style", "display:inline-block!important");
        })        
    })
    function LoginOnClick()
    {    
        $("form").attr("target", "");
    } 
    </script>
    
