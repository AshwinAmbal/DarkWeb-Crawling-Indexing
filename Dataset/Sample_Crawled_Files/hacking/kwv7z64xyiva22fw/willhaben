    <script>
        var api;
        jQuery(document).ready(function () {
            api = jQuery("#gallery").unitegallery();
        });

        function customXitiClickSlider(href, page) {
            var tealiumData = {"tmsData":{"event_name":"homepage","environment":"web","source":"Web","page_type":"Home"}};
            if (tealiumData && tealiumData.tmsData) {
                var searchTerm = page.split("::");
                var tempSearchTerm = searchTerm.length > 1 ? searchTerm[1] : "";
                if (tempSearchTerm === "Mode_Accessoires" && searchTerm.length > 2) {
                    tempSearchTerm = tempSearchTerm.concat("::", searchTerm[2]);
                }
                tealiumData.tmsData.search_terms = tempSearchTerm
            }
            window.IADLIB.tealium.trackClickEvent(tealiumData, "N", "trends");
            window.location.href = href;
        }
    </script>


