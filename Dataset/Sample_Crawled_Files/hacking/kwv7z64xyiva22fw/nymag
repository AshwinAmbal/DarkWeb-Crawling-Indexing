    border: none;
    clip: rect(0 0 0 0);
    height: 1px;
    margin: -1px;
    overflow: hidden;
    padding: 0;
    position: absolute;
    width: 1px;
  }

.nav-search-button.closed .magnifier {
    display: block;
  }

.nav-search-button.closed .close-x {
    display: none;
  }

.nav-search-button.open {
  width: 44px;
}

.nav-search-button.open .magnifier {
    display: none;
  }

