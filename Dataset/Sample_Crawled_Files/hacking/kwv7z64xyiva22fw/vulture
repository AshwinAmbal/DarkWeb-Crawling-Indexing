    fill: #000;
  }

.nav-search-button-trigger:active svg path[fill='#111'],
  .nav-search-button-trigger:focus svg path[fill='#111'],
  .nav-search-button-trigger:hover svg path[fill='#111'],
  .nav-search-button  {
    fill: #000;
  }

