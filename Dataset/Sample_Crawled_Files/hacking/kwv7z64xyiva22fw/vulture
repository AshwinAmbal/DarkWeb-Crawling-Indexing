    background-image: linear-gradient(to bottom, rgba(255, 255, 255, 0), #fff);
    border-bottom: 4em solid #fff;
    bottom: 0;
    content: '';
    height: 16em;
    left: -3px;
    position: absolute;
    width: calc(6px + 100%);
    z-index: -1;
  }
