        //<![CDATA[
        function socialLogin(url) {
            var sX = screen.width / 2 - 580 / 2;
            var sY = screen.height / 2 - 400 / 2;
            sLogin = window.open(url, 'facebook-login', 'width=580,height=400,left=' + sX + ',top=' + sY);
            if (window.focus) {
                sLogin.focus();
            }
        }
