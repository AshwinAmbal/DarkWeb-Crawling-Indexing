<script>
    require([
        'jquery'
    ], function ($) {
        $('#minisearch-label-select').click(function () {
            $(this).toggleClass('active');
        });

        $(".choose-seach-type").change(function () {
            var value = $(this).val();
            if (value && value != 'all') {
                $('.search-type .checkbox').removeAttr('checked');
                $('.search-type #type-' + value).attr('checked', 'checked');
            } else {
                $('.search-type .checkbox').removeAttr('checked', 'checked');
            }
        });
    });
</script>    <div class="custom-block"></div>    </div>
    
