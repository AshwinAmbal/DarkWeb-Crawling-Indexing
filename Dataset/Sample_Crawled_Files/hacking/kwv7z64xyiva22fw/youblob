<script type="text/javascript">
    require([
        'jquery',
        'Smartwave_Blueprint/js/jquery.qtip.min'
    ], function ($) {
        $(document).ready(function () {
            var blueprint_category_active = $('#blueprint-category-active').val();
            var product_category_active = $('#product-category-active').val();
            var service_category_active = $('#service-category-active').val();
            if (blueprint_category_active === '1' && product_category_active === '0' && service_category_active === '0') {
                $('#blueprint-category-sidebar').show();
                $('#product-category-sidebar').hide();
                $('#service-category-sidebar').hide();
				
				$('.blueprint-product-service-title > li').removeClass('active');
                $('.blueprint-category-label').addClass('active');
            }
            if (blueprint_category_active === '0' && product_category_active === '1' && service_category_active === '0') {
                $('#blueprint-category-sidebar').hide();
                $('#product-category-sidebar').show();
                $('#service-category-sidebar').hide();
				
				$('.blueprint-product-service-title > li').removeClass('active');
                $('.product-category-label').addClass('active');
            }
            if (blueprint_category_active === '0' && product_category_active === '0' && service_category_active === '1') {
                $('#blueprint-category-sidebar').hide();
                $('#product-category-sidebar').hide();
                $('#service-category-sidebar').show();
				
				$('.blueprint-product-service-title > li').removeClass('active');
                $('.service-category-label').addClass('active');
            }
			if($('.blueprint-product-service-title > li.calendar-category-label.active').length){
				$('#blueprint-category-sidebar').hide();
                $('#product-category-sidebar').hide();
                $('#service-category-sidebar').hide();
			}
			
			if(!$('.blueprint-product-service-title > li.active').length){
				$('.blueprint-category-label').addClass('active');
			}
			
            $('.c-sidebar--categories').on('mouseover', '.category-qtip', function () {
                if ($(this).data('category-description')) {
                    $(this).qtip({
                        content: {
                            text: $(this).data('category-description')
                        },
                        style: {
                            classes: 'qtip-light qtip-rounded'
                        },
                        show: {
                            event: 'mousemove'
                        },
                        hide: {
                            event: 'mouseout'
                        }
                    });
                }
            });
            $('.c-sidebar--categories').on('click', '.switch-category', function () {
                if ($(this).data('switch-category') == 'product-category-sidebar') {
                    $('#blueprint-category-sidebar').hide();
                    $('#product-category-sidebar').show();
                    $('#service-category-sidebar').hide();
                } else if ($(this).data('switch-category') == 'service-category-sidebar') {
                    $('#blueprint-category-sidebar').hide();
                    $('#product-category-sidebar').hide();
                    $('#service-category-sidebar').show();
                } else if ($(this).data('switch-category') == 'blueprint-category-sidebar') {
                    $('#blueprint-category-sidebar').show();
                    $('#product-category-sidebar').hide();
                    $('#service-category-sidebar').hide();
                }
            });
			
			$('.blueprint-product-service-title').on('click', '.switch-category', function () {
				$('.blueprint-product-service-title > li').removeClass('active');
				$(this).parent().addClass('active');
                if ($(this).data('switch-category') == 'product-category-sidebar') {
                    $('#blueprint-category-sidebar').hide();
                    $('#product-category-sidebar').show();
                    $('#service-category-sidebar').hide();
                } else if ($(this).data('switch-category') == 'service-category-sidebar') {
                    $('#blueprint-category-sidebar').hide();
                    $('#product-category-sidebar').hide();
                    $('#service-category-sidebar').show();
                } else if ($(this).data('switch-category') == 'blueprint-category-sidebar') {
                    $('#blueprint-category-sidebar').show();
                    $('#product-category-sidebar').hide();
                    $('#service-category-sidebar').hide();
                }
				else if ($(this).data('switch-category') == 'calendar-category-sidebar') {
                    $('#blueprint-category-sidebar').hide();
                    $('#product-category-sidebar').hide();
                    $('#service-category-sidebar').hide();
                }
            });
			
			var windowWidth = $(window).width();
			if(windowWidth < 768 && $('.sidebar-additional-box .blueprint-product-service-container-sidebar').length > 0){
				$( ".nav-sections-items > div" ).each(function( index ) {
					if($( this ).attr('id') == 'store.menu'){
						$('.blueprint-product-service-container-sidebar').appendTo($(this));
					}
				});
			}
			
        });
		
		$(window).resize(function () {
			var windowWidth = $(window).width();
			if(windowWidth < 768 && $('.sidebar-additional-box .blueprint-product-service-container-sidebar').length > 0){
				$( ".nav-sections-items > div" ).each(function( index ) {
					if($( this ).attr('id') == 'store.menu'){
						$('.blueprint-product-service-container-sidebar').appendTo($(this));
					}
				});
			}
			else{
				if($('.nav-sections-items .blueprint-product-service-container-sidebar').length > 0){
					$('.blueprint-product-service-container-sidebar').prependTo($('.sidebar-additional-box'));
				}
			}
		});
    });
</script>	</div>    <div class="block block-wishlist" data-bind="scope: 'wishlist'">
