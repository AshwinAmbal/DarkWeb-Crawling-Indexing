  <section class="article__body">
    <div class="article__content" data-editable="content" itemprop="mainEntityOfPage">
          <figure
          data-uri="slate.com/_components/image/instances/cjs6roo8a003ysmktawmb1trj@published"
          class="image image--center "
          data-editable="imageInfo"
        >
        
                <div class="lazy-container" style="padding-bottom: 66.66666666666666%;">
                  <img class="image__src" data-normal="https://compote.slate.com/images/05b5aca9-d698-49fd-b7dc-0cc005be8fb7.jpeg?width&#x3D;780&amp;height&#x3D;520&amp;rect&#x3D;1881x1254&amp;offset&#x3D;0x0" alt="An image of a mocked-up Google Map with a bunch of dots piled on top of/next to each other." />
                  <noscript>
                    <img src="https://compote.slate.com/images/05b5aca9-d698-49fd-b7dc-0cc005be8fb7.jpeg?width&#x3D;780&amp;height&#x3D;520&amp;rect&#x3D;1881x1254&amp;offset&#x3D;0x0" alt="An image of a mocked-up Google Map with a bunch of dots piled on top of/next to each other." />
                  </noscript>
