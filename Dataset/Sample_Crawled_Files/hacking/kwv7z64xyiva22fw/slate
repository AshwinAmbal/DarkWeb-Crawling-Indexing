          <div data-uri="slate.com/_components/slate-ad/instances/teadsBackfillTablet"
        data-placeholder="settings"
            class="ad ad--tabletPortraitOnly"
            data-page-type="article"
            data-type="outstream-backfill-tablet"
            data-sizes="728x90,300x250,1x1,1x3"
