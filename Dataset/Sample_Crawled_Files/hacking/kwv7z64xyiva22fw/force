                    function noenter(e, str){     
                        if(window.event){                    
                            key = window.event.keyCode; //IE            
                        } else{ 
                            key = e.which;              //firefox               
                        }
                        if(key == 13) {
                            var ele=document.getElementById(str);       
                            ele.click();                    
                            return false;              
                        } else{                   
                            return true;              
                        }         
                    }                    
                    var startTimestamp = +new Date();                                                      
                </script>
