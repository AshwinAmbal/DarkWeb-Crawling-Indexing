<script type="text/javascript">
//<![CDATA[
var roomKeyExecute = function() {
		if ('VIEGIGI' != '' && 'VIEGIGI' != null) {
			if (jQuery('input[name="searchType"]').val() == 'ALL' && (BRAND.id == 'HP' || BRAND.id == 'HT' || BRAND.id == 'HW' || BRAND.id == 'QQ' || BRAND.id == 'PY' || BRAND.id == 'RU' || BRAND.luxuryService)){
				setRoomKeyParameters('frmfindHotel', 'c8fb3636c239e34248d19fc92fbe6d6e', '/skins/common/img/logo_brand-GI.gif', 'GI_GW_Home', '', 'https://secure3.hilton.com/en_US/gi/reservation/book.htm?ctyhocn=VIEGIGI&internalDeepLinking=true');
			} else if (jQuery('input[name="searchType"]').val() == 'PROP' && (BRAND.id == 'HT' || BRAND.id == 'QQ' || BRAND.id == 'PY' || BRAND.id == 'RU' || BRAND.luxuryService)) {
				// get the ctyhocn from the "Home2 Suites location" dropdown widget as we need to pass selected option to room key
				var ctyhocnDropdownVal;
				if(BRAND.id != 'HT'){
					ctyhocnDropdownVal = jQuery('#frmfindHotel').find('select[id=hotelSearchOneBoxCtyhocn]').val();
				}else{
					ctyhocnDropdownVal = jQuery('#frmfindHotel').find('input[id=ctyhocn]').val();
				}
				var ctyhocn = ctyhocnDropdownVal != null ? ctyhocnDropdownVal : 'VIEGIGI';
				setRoomKeyParameters('frmfindHotel', 'c8fb3636c239e34248d19fc92fbe6d6e', '/skins/common/img/logo_brand-GI.gif', 'GI_GW_Home', ctyhocn, 'https://secure3.hilton.com/en_US/gi/reservation/book.htm?ctyhocn=VIEGIGI&internalDeepLinking=true');
			} else if (!(jQuery('input[name="searchType"]').val() == 'PROP' && BRAND.luxuryService)) {
				setRoomKeyParameters('frmfindHotel', 'c8fb3636c239e34248d19fc92fbe6d6e', '/skins/common/img/logo_brand-GI.gif', 'GI_GW_Home', 'VIEGIGI', 'https://secure3.hilton.com/en_US/gi/reservation/book.htm?ctyhocn=VIEGIGI&internalDeepLinking=true');
			}
		} else {
			if (jQuery('input[name="searchType"]').val() == 'PROP' && (BRAND.id == 'HT' || BRAND.id == 'QQ' || BRAND.id == 'PY' || BRAND.id == 'RU' || BRAND.luxuryService)) {
				// get the ctyhocn from the "Home2 Suites location" dropdown widget as we need to pass selected option to room key
				var ctyhocn;
				if(BRAND.id != 'HT'){
					ctyhocn = jQuery('#frmfindHotel').find('select[id=hotelSearchOneBoxCtyhocn]').val();
				}else{
					ctyhocn = jQuery('#frmfindHotel').find('input[id=ctyhocn]').val();
				}
				setRoomKeyParameters('frmfindHotel', 'c8fb3636c239e34248d19fc92fbe6d6e', '/skins/common/img/logo_brand-GI.gif', 'GI_GW_Home', ctyhocn, '');
			} else if (jQuery('input[name="searchType"]').val() == 'ALL' || BRAND.id == 'HT' || BRAND.id == 'QQ' || BRAND.id == 'PY' || BRAND.id == 'RU' || BRAND.luxuryService) {
				setRoomKeyParameters('frmfindHotel', 'c8fb3636c239e34248d19fc92fbe6d6e', '/skins/common/img/logo_brand-GI.gif', 'GI_GW_Home', '', '');
			}
		}
	};
//]]>
</script>

